// Generator to use
// (Default : ninja)
LABS_GENERATOR:STRING=ninja

// Should paths be relatives ?
// (Default : False)
LABS_RELATIVE_PATHS:BOOL=False

// Source directory path. labs_build.py should be in this directory.
// (Default : {{test_dir}})
LABS_SOURCE_PATH:FILEPATH={{test_dir}}

// A test var
// ...Documentation continued
VARTEST:STRING(USER)=1

// var 1
// (Default : 4)
var1:STRING=5

// var 2
// (Default : 2)
var2:INT=3

// var1 + var2
// (Default : $(var1)$(var2))
var3:INT=$(var1)$(var2)

// var1 + var2 + var3
// (Default : $(var1)$(var2)$(var3))
var4:INT=$(var1)$(var2)$(var3)

// var1 + var2 + var3
// (Default : 20)
var5:INT=$(var3)$(var1)$(VARTEST)

